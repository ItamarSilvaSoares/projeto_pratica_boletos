{
  "name": "projeto-boleto",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server.js",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/db/prisma/seeders/seed.ts",
    "schema": "./src/db/prisma/schema.prisma"
  },
  "scripts": {
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/index.ts\"",
    "build": "tsc",
    "test": "mocha ./tests/**/*$NAME*.test.ts --exit",
    "debug": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "Itamar Soares",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.5.0",
    "sinon": "^14.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1"
  }
}