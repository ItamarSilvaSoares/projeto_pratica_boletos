    "joi": "^17.6.4",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
